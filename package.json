{
  "name": "chat",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "type-check": "tsc --noEmit --skipLibCheck",
    "prettier:format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\" --ignore-path \".prettierignore\"\n",
    "fix": "yarn lint --fix && yarn prettier:format",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "@apollo/client": "^3.8.7",
    "@jumpn/utils-graphql": "^0.6.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "graphql": "^16.8.1",
    "lodash.debounce": "^4.0.8",
    "phoenix": "^1.7.10",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-svg": "^13.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@types/absinthe__socket": "^0.2.6",
    "@types/absinthe__socket-apollo-link": "^0.2.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-universe": "^12.0.0",
    "prettier": "^3.0.3",
    "react-native-dotenv": "^3.4.9",
    "react-native-svg-transformer": "^1.1.0",
    "typescript": "^5.2.2"
  },
  "private": true
}
